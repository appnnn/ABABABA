<UserControl x:Class="WpfApp1.DashboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="ArrivedButton" Content="Arrived" Width="120" Height="40" Click="ArrivedButton_Click"/>
            <Button x:Name="ExitButton" Content="Exit" Click="ExitButton_Click" IsEnabled="False"/>
        </StackPanel>
    </Grid>
</UserControl>
